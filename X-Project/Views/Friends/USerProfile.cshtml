@model IEnumerable<X_Project.ModelData.WishListItem>

@{
    ViewBag.Title = "USerProfile";
}
    <button style="display:inline; float:left" type="button" class="btn btn-primary" aria-label="Left Align" onclick="location.href='@Url.Action("Index", "Friends")'">
        <span style="display:inline;" class="glyphicon glyphicon-arrow-left" aria-hidden="true">Back</span>
    </button>
<div class="MyContainer">
    <div class="item-div">
        @*<a href="" class="glyphicon glyphicon-arrow-left">Go Back</a>*@
        <h1 style="font-family:fantasy; display:inline;">Wishlist</h1>
    </div>
</div>

<div class="MyContainer">
    @foreach (var item in Model.OrderByDescending(x => x.Time))
            {
        <div class="item-div">
            <div class="item ">
                <img src="@item.Image" alt="item" />
                <h2>@item.Title</h2>
                <a href=@item.WishUrl style="text-decoration: underline;">View item</a>
            </div>
        </div>
    }
</div>





@model X_Project.ModelData.ViewModel
    @if (Model._WishListItem.Count() == 0)
    {
        <div class="MyContainer">
            <div class="item-div">
                <h1 style="font-family: cursive;">No wishlist yet!</h1>
            </div>
        </div>
    }
<div class="MyContainer">
    @foreach (var item in Model._WishListItem.OrderByDescending(x => x.Time))
            {
        <div class="item-div">
            <div class="item">
                <input type="hidden" value="@item.Id" id="WishId"/>
                <img src="@item.Image" alt="item" />
                <h2>@item.Title</h2>
                <a href=@item.WishUrl style="text-decoration: underline;">View item</a>
                <button type="button" id="deleteWishbtn" class="itembutton">Delete</button>
                @*@Html.ActionLink("DeleteWish", "DeleteWish", new {id = @item.Id, @class = "btn btn-default" })*@
            </div>
        </div>
    }
</div>
